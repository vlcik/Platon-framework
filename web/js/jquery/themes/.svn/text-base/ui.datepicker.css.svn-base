



/* web datepicker */


/* Main Style Sheet for jQuery UI date picker */
.webDatePicker, #wrap-web .ui-datepicker-inline, #ui-datepicker-div.webDatePicker {
	/*resets*/margin: 0; padding: 0; border: 0; outline: 0; line-height: 1.3; text-decoration: none; font-size: 100%; list-style: none;
	font-family: Verdana, Arial, sans-serif;
	background: url(images/bg-datepicker.png) 0 0 no-repeat;
	font-size: 1.1em;
	width:210px !important;
	height:152px ;
	padding:37px 5px 5px 13px;
	/*border: 4px solid #fff;
	width: 15.5em;
	padding: 2.5em .5em .5em .5em;*/
	position: relative;
}
.webDatePicker, .ui-datepicker-div, #ui-datepicker-div.webDatePicker {
	z-index: 9999; /*must have*/
	display: none;
}
.webDatePicker .ui-datepicker-inline {
	float: left;
	display: block;
}
.webDatePicker .ui-datepicker-control {
	display: none;
}
.webDatePicker .ui-datepicker-current {
	display: none;
}
.webDatePicker .ui-datepicker-next, .webDatePicker .ui-datepicker-prev {
	position: absolute;
	left: 1.4em;
	top: 1.2em;
	background: #111111 url(images/111111_40x100_textures_02_glass_40-blue.png) 0 50% repeat-x;
}
.webDatePicker .ui-datepicker-next {
	left: 17.5em;
}
.webDatePicker .ui-datepicker-next:hover, .webDatePicker .ui-datepicker-prev:hover {
	background: #1c1c1c url(images/1c1c1c_40x100_textures_02_glass_55.png) 0 50% repeat-x;
}
.webDatePicker .ui-datepicker-next a, .webDatePicker .ui-datepicker-prev a {
	text-indent: -999999px;
	width: 1.3em;
	height: 1.4em;
	display: block;
	font-size: 1em;
	background: url(images/ededed_7x7_arrow_left.gif) 50% 50% no-repeat;
	border: 1px solid #777777;
	cursor: pointer;
}
.webDatePicker .ui-datepicker-next a {
	background: url(images/ededed_7x7_arrow_right.gif) 50% 50% no-repeat;
}
.webDatePicker .ui-datepicker-prev a:hover {
	background: url(images/ffffff_7x7_arrow_left.gif) 50% 50% no-repeat;
}
.webDatePicker .ui-datepicker-next a:hover {
	background: url(images/ffffff_7x7_arrow_right.gif) 50% 50% no-repeat;
}
.webDatePicker .ui-datepicker-prev a:active {
	background: url(images/222222_7x7_arrow_left.gif) 50% 50% no-repeat;
}
.webDatePicker .ui-datepicker-next a:active {
	background: url(images/222222_7x7_arrow_right.gif) 50% 50% no-repeat;
}
.webDatePicker .ui-datepicker-header select {
	border: 1px solid #777777;
	color: #e3e3e3;
	background: #08336a;
	font-size: 1em;
	line-height: 1.4em;
	position: absolute;
	top: 1.3em;
	margin: 0 1.9em !important;
}
.webDatePicker .ui-datepicker-header option:focus, .webDatePicker .ui-datepicker-header option:hover {
	background: #1c1c1c;
}
.webDatePicker .ui-datepicker-header select.ui-datepicker-new-month {
	width: 7em;
	left: 2.2em;
}
.webDatePicker .ui-datepicker-header select.ui-datepicker-new-year {
	width: 5em;
	left: 9.4em;
}
.webDatePicker table.ui-datepicker {
	width: 15.5em;
	text-align: right;
}

.webDatePicker table.ui-datepicker td a {
	text-align: center;
	vertical-align: middle;
}

.webDatePicker table.ui-datepicker td a {
	padding: .1em .3em .1em .3em;
	display: block;
	color: #e3e3e3;
	background: #111111 url(images/111111_40x100_textures_02_glass_40-blue.png) 0 50% repeat-x;
	cursor: pointer;
	border: 1px solid #f9f9f9;
}
.webDatePicker table.ui-datepicker td a:hover {
	border: 1px solid #000000;
	color: #ffffff;
	background: #1c1c1c url(images/1c1c1c_40x100_textures_02_glass_55.png) 0 50% repeat-x;
}
.webDatePicker table.ui-datepicker td a:active {
	border: 1px solid #cccccc;
	color: #222222;
	background: #ffffff url(images/ffffff_40x100_textures_01_flat_65.png) 0 50% repeat-x;
}
.webDatePicker table.ui-datepicker .ui-datepicker-title-row td {
	padding: .3em 0;
	text-align: center;
	font-size: .9em;
	color: #222222;
	text-transform: uppercase;
}
.webDatePicker table.ui-datepicker .ui-datepicker-title-row td a {
	/*color: #222222;*/
	color: white;
}
.webDatePicker .ui-datepicker-cover {
	display: none;
	display/**/: block;
	position: absolute;
	z-index: -1;
	filter: mask();
	top: -4px;
	left: -4px;
	width: 193px;
	height: 200px;
}

.webDatePicker .ui-datepicker-days-cell {
padding-bottom:2px;	
}


